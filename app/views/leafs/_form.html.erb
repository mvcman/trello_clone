<%= form_with(model: [user, leaf], class: "") do |form| %>
    <div class="my-2">
        <%= form.datetime_local_field :start_date, class: "" %>
    </div>
    <div class="my-2">
        <%= form.datetime_local_field :end_date, class: "" %>
    </div>
    <div class="my-2">
        <%= form.submit "Create", class: "" %>
    </div>
<% end %>